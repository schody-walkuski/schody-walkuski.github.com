  %body
    #wrapper
      #container
        #box
          #content
            #header
              %h1 Schody Wałkuski
              %p
                Zbigniew Wałkuski, Konarzyce, Łomżyńska 36, 18-400 Łomża, +48 505 045 899, kontakt (at) schody-walkuski.pl
            #gallery
              - (1..15).each do |number|
                .image_wrapper
                  %a{ :class => :fancybox, :rel => :gallery, :href => "images/#{number}.jpg" }
                    %img{ :src => "images/#{number}_thumb.jpg", :border => 0 }
              %span
