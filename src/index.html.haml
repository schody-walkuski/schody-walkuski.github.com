!!!5
%html
  %head
    %title schody-walkuski.pl - przepiękne schody i drzwi z drewna
    %meta{ :'http-equiv' => 'Content-Type', :content =>'text/html; charset=UTF-8' }/
    %meta{ :name => 'google-site-verification', :content => 'eBsWf0fHKjRMKPFZhBbDjrbpRkCwnNXirkwB_s0YWwM' }/
    %meta{ :name => 'description', :content => 'schody-walkuski.pl - przepiękne schody i drzwi z drewna' }/
    %link{ :rel => 'stylesheet', :href => 'styles/jquery.tipsy.css', :type => 'text/css' }/
    %link{ :rel => 'stylesheet', :href => 'styles/jquery.fancybox.css', :type => 'text/css' }/
    %link{ :rel => 'stylesheet', :href => 'styles/application.css', :type => 'text/css' }/
    %script{ :type => 'text/javascript', :src => 'js/jquery.min.js' }
    %script{ :type => 'text/javascript', :src => 'js/jquery.tipsy.js' }
    %script{ :type => 'text/javascript', :src => 'js/jquery.easing.js' }
    %script{ :type => 'text/javascript', :src => 'js/jquery.mousewheel.js' }
    %script{ :type => 'text/javascript', :src => 'js/jquery.fancybox.js' }
